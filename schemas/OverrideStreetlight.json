{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "OverrideStreetlightRequest",
    "type": "object",
    "properties": {
        "contactorId": {
            "type": "integer",
            "minimum": 1,
            "maximum": 4
        },
        "action": {
            "type": "string",
            "enum": [
                    "On",
                    "Off",
                    "Suspend"
                    ]
        }
    },
    "additionalProperties": false,
    "required": [
        "action"
    ]
}
